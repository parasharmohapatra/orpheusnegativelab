     /* Global Styles */
     QMainWindow {
       background-color: #121212;
       font-family: 'Segoe UI', 'Roboto', sans-serif;
     }
     QWidget {
       font-family: 'Segoe UI', 'Roboto', sans-serif;
     }
     
     /* Sidebar */
     #sidebar {
       background-color: #1e1e1e;
       min-width: 360px;
       max-width: 360px;
       padding: 20px;
       border-right: 1px solid #2c2c2c;
     }
     
     /* Main Content Area */
     #main_content {
       background-color: #121212;
       padding: 32px;
     }
     
     /* Labels */
     QLabel {
       color: #e0e0e0;
       font-size: 13px;
       font-weight: 400;
     }
     
     /* Title */
     #titleLabel {
       color: #ffffff;
       font-size: 28px;
       font-weight: 600;
       padding: 0px 0px;
       margin: 8px 0px 22px 0px;
     }
     
     /* Buttons */
     QPushButton {
       background-color: #2d2d2d;
       color: #ffffff;
       border: none;
       border-radius: 6px;
       padding: 10px 16px;
       font-size: 13px;
       font-weight: 500;
       min-width: 120px;
       outline: none;
     }
     QPushButton:hover {
       background-color: #ff6e40;
       color: #ffffff;
     }
     QPushButton:pressed {
       background-color: #ff5722;
     }
     
     /* Transform Buttons */
     #rotateButton, #flipButton, #cropButton {
       min-width: 80px;
       max-width: 100px;
       padding: 10px 8px;
     }
     
     /* Sliders */
     QSlider {
       height: 32px;
     }
     QSlider::groove:horizontal {
       border: none;
       height: 4px;
       background: #333333;
       border-radius: 2px;
     }
     QSlider::handle:horizontal {
       background: #ff6e40;
       border: 2px solid #ff6e40;
       width: 16px;
       height: 16px;
       margin: -6px 0;
       border-radius: 8px;
     }
     QSlider::handle:horizontal:hover {
       background: #ff5722;
       border: 2px solid #ff5722;
     }
     
     /* Separator Lines */
     Line {
       background-color: #2c2c2c;
       height: 1px;
       margin: 10px 0;
     }
     
     /* Status Bar */
     QStatusBar {
       background-color: #1e1e1e;
       color: #b0b0b0;
       padding: 0 16px;
       border-top: 1px solid #2c2c2c;
       min-height: 30px;
     }
     
     /* Scroll Area */
     QScrollArea {
       border: none;
       background-color: transparent;
     }
     QScrollBar:vertical {
       background: #1e1e1e;
       width: 8px;
       margin: 0;
     }
     QScrollBar::handle:vertical {
       background: #333333;
       min-height: 30px;
       border-radius: 4px;
     }
     QScrollBar::handle:vertical:hover {
       background: #ff6e40;
     }
     QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
       height: 0px;
     }
     QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
       background: none;
     }
     QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
       background: none;
     }
     
     /* Image Label */
     #image_label {
       background-color: #1e1e1e;
       border: 1px solid #1e1e1e;
       border-radius: 0px;
       min-width: 400px;
       min-height: 300px;
       padding: 4px;
     }
     
     /* Progress Bar */
     QProgressBar {
       width: 150px;
       height: 4px;
       margin-right: 20px;
       background-color: #333333;
       border-radius: 2px;
       border: none;
       text-align: center;
     }
     QProgressBar::chunk {
       background-color: #ff6e40;
       border-radius: 2px;
     }
   </string>
